<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="415dp"
        android:layout_height="280dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/home_top_rect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/logo_image"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:background="@drawable/text_logo"
        app:layout_constraintStart_toEndOf="@+id/image_1"
        app:layout_constraintTop_toTopOf="@+id/image_1" />

    <View
        android:id="@+id/image_1"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/ic_menu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/ellipse_1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="32dp"
        android:background="@drawable/ic_account"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/logo_image" />

    <TextView
        android:id="@+id/login_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/jost"
        android:gravity="top"
        android:text="Hello,"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/image_1"
        app:layout_constraintTop_toBottomOf="@+id/image_1" />

    <TextView
        android:id="@+id/login_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/jost"
        android:gravity="top"
        android:text="Siddhesh Desai"
        android:textColor="#FFFFFF"
        android:textSize="29sp"
        app:layout_constraintStart_toStartOf="@+id/login_text"
        app:layout_constraintTop_toBottomOf="@+id/login_text" />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="318dp"
        android:layout_height="137dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/home_box"
        android:elevation="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_text1" />

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="13dp"
        android:layout_height="137dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/home_box_side"
        android:elevation="5dp"
        app:layout_constraintStart_toStartOf="@+id/rectangle_2"
        app:layout_constraintTop_toTopOf="@+id/rectangle_2" />

    <TextView
        android:id="@+id/you_have_ce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:elevation="10dp"
        android:fontFamily="@font/jost"
        android:gravity="top"
        android:text="You have censused for:"
        android:textColor="#00084B"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/rectangle_3"
        app:layout_constraintTop_toTopOf="@+id/rectangle_2" />

    <TextView
        android:id="@+id/names"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="6dp"
        android:elevation="10dp"
        android:fontFamily="@font/jost"
        android:gravity="top"
        android:text="1. Siddhesh Desai \n2. Shreeji Desai"
        android:textColor="#A94100"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/you_have_ce"
        app:layout_constraintTop_toBottomOf="@+id/you_have_ce" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="28dp"
        android:elevation="5dp"
        android:fontFamily="@font/jost"
        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowRadius="15"
        android:text="5"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="86sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_2"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_2"
        app:layout_constraintTop_toTopOf="@+id/rectangle_2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/members"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:elevation="10dp"
        android:fontFamily="@font/jost"
        android:gravity="top"
        android:text="MEMBERS"
        android:textColor="#00084B"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/some_id"
        app:layout_constraintEnd_toEndOf="@+id/some_id"
        app:layout_constraintStart_toStartOf="@+id/some_id" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button"
        android:layout_width="74dp"
        android:layout_height="23dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/home_box_btn"
        android:backgroundTintMode="add"
        android:elevation="100dp"
        android:fontFamily="@font/jost"
        android:padding="0dp"
        android:text="View All"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="9sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/names"
        app:layout_constraintTop_toBottomOf="@+id/names" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/box_add_btn"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/home_box_btn"
        android:backgroundTintMode="add"
        android:elevation="100dp"
        android:fontFamily="@font/jost"
        android:padding="0dp"
        android:text="Add Member"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="9sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/button" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/add_btn"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/btn_bg"
        android:drawableTop="@drawable/ic_add_person"
        android:fontFamily="@font/jost"
        android:padding="25dp"
        android:text="Add a Member"
        android:textSize="15dp"
        android:textAllCaps="false"
        android:textStyle="bold"
        app:flow_horizontalAlign="center"
        app:flow_verticalAlign="center"
        app:layout_constraintStart_toStartOf="@+id/rectangle_2"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/btn_bg"
        android:drawableTop="@drawable/ic_add_person"
        android:fontFamily="@font/jost"
        android:padding="25dp"
        android:text="Edit Member Data"
        android:textAllCaps="false"
        android:textSize="15dp"
        android:textStyle="bold"
        app:flow_horizontalAlign="center"
        app:flow_verticalAlign="center"
        app:layout_constraintStart_toStartOf="@+id/add_btn"
        app:layout_constraintTop_toBottomOf="@+id/add_btn" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button7"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:background="@drawable/btn_bg"
        android:drawableTop="@drawable/ic_add_person"
        android:fontFamily="@font/jost"
        android:padding="25dp"
        android:text="View all Members"
        android:textAllCaps="false"
        android:textSize="15dp"
        android:textStyle="bold"
        app:flow_horizontalAlign="center"
        app:flow_verticalAlign="center"
        app:layout_constraintEnd_toEndOf="@+id/button5"
        app:layout_constraintTop_toTopOf="@+id/button6" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button5"
        android:layout_width="150dp"
        android:layout_height="115dp"
        android:background="@drawable/btn_bg"
        android:drawableTop="@drawable/ic_add_person"
        android:fontFamily="@font/jost"
        android:padding="25dp"
        android:text="Remove a Member"
        android:textSize="15dp"
        android:textAllCaps="false"
        android:textStyle="bold"
        app:flow_horizontalAlign="center"
        app:flow_verticalAlign="center"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_2"
        app:layout_constraintTop_toTopOf="@+id/add_btn" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button8"
        android:layout_width="321dp"
        android:layout_height="40dp"
        android:background="@drawable/btn2_bg"
        android:fontFamily="@font/jost"
        android:text="CHANGE ADDRESS"
        android:textColor="#FFFFFF"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button7"
        app:layout_constraintStart_toStartOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/button6" />


</androidx.constraintlayout.widget.ConstraintLayout>